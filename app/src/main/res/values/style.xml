<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="defaultViewStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="spinnerStyle" parent="defaultViewStyle">
        <item name="smsp_errorTextColor">@color/error_color</item>
        <item name="smsp_floatingLabelColor">@color/floating_color</item>
        <item name="smsp_underlineColor">@color/underline_color</item>
        <item name="smsp_errorTextSize">13sp</item>
        <item name="smsp_itemSize">14sp</item>
        <item name="smsp_hintSize">14sp</item>
    </style>
</resources>