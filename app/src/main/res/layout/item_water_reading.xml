<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_marginTop="4dp">
    <TextView
        android:id="@+id/period_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/period"
        android:fontFamily="@font/signika_negative_bold"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        />
    <androidx.cardview.widget.CardView
        android:id="@+id/card_period"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        style="@style/CardStyleSecond"
        app:layout_constraintTop_toBottomOf="@id/period_text"
        app:layout_constraintStart_toStartOf="@id/period_text"
        >
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:padding="8dp"
            android:layout_height="wrap_content">
    <TextView
        android:id="@+id/date_ot_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/date_ot_colon"
        style="@style/ChildText"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />
    <TextView
        android:id="@+id/date_do_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/date_do_colon"
        style="@style/ChildText"
        app:layout_constraintTop_toBottomOf="@id/date_ot_text"
        app:layout_constraintStart_toStartOf="@id/date_ot_text"
        />
    <TextView
        android:id="@+id/days_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/days_colon"
        style="@style/ChildText"
        app:layout_constraintTop_toBottomOf="@id/date_do_text"
        app:layout_constraintStart_toStartOf="@id/date_do_text"
        />
            <TextView
                android:id="@+id/date_ot"
                style="@style/SecondText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                app:layout_constraintBottom_toBottomOf="@id/date_ot_text"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@id/date_ot_text"
                tools:text="20-03-2023" />
            <TextView
                android:id="@+id/date_do"
                style="@style/SecondText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                app:layout_constraintBottom_toBottomOf="@id/date_do_text"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@id/date_do_text"
                tools:text="21-03-2023" />
            <TextView
                android:id="@+id/days"
                style="@style/SecondText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                app:layout_constraintBottom_toBottomOf="@id/days_text"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@id/days_text"
                tools:text="500.00" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>
    <TextView
        android:id="@+id/pokaz_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/reading"
        android:fontFamily="@font/signika_negative_bold"
        app:layout_constraintTop_toBottomOf="@id/card_period"
        app:layout_constraintStart_toStartOf="@id/card_period"
        />
    <androidx.cardview.widget.CardView
        android:id="@+id/card_pokaz"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        style="@style/CardStyleSecond"
        app:layout_constraintTop_toBottomOf="@id/pokaz_text"
        app:layout_constraintStart_toStartOf="@id/pokaz_text"
        >
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:padding="8dp"
            android:layout_height="wrap_content">
    <TextView
        android:id="@+id/last_colon_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/last_colon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        style="@style/ChildText"
        />

    <TextView
        android:id="@+id/current_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/current_colon"
        style="@style/ChildText"
        app:layout_constraintTop_toBottomOf="@id/last_colon_text"
        app:layout_constraintStart_toStartOf="@id/last_colon_text"
        />
    <TextView
        android:id="@+id/cubs_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/cubs_colon"
        style="@style/ChildText"
        app:layout_constraintTop_toBottomOf="@id/current_text"
        app:layout_constraintStart_toStartOf="@id/current_text"
        />
            <TextView
                android:id="@+id/last"
                style="@style/SecondText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                app:layout_constraintBottom_toBottomOf="@id/last_colon_text"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@id/last_colon_text"
                tools:text="500.00" />
            <TextView
                android:id="@+id/current"
                style="@style/SecondText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                app:layout_constraintBottom_toBottomOf="@id/current_text"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@id/current_text"
                tools:text="500.00" />

            <TextView
                android:id="@+id/cubs"
                style="@style/SecondText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                app:layout_constraintBottom_toBottomOf="@id/cubs_text"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@id/cubs_text"
                tools:text="500.00" /></androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>
    <TextView
        android:id="@+id/avg_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/average"
        android:fontFamily="@font/signika_negative_bold"
        app:layout_constraintTop_toBottomOf="@id/card_pokaz"
        app:layout_constraintStart_toStartOf="@id/card_pokaz"
        />
    <androidx.cardview.widget.CardView
        android:id="@+id/card_avg"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        style="@style/CardStyleSecond"
        android:background="?attr/colorSecondaryVariant"
        app:layout_constraintTop_toBottomOf="@id/avg_text"
        app:layout_constraintStart_toStartOf="@id/avg_text"
        >
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:padding="8dp"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/pok_ot_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/last_colon"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                style="@style/ChildText"
                />

            <TextView
                android:id="@+id/pok_do_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/current_colon"
                style="@style/ChildText"
                app:layout_constraintTop_toBottomOf="@id/pok_ot_text"
                app:layout_constraintStart_toStartOf="@id/pok_do_text"
                />
            <TextView
                android:id="@+id/qty_cube_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/qty_cube_colon"
                style="@style/ChildText"
                app:layout_constraintTop_toBottomOf="@id/pok_do_text"
                app:layout_constraintStart_toStartOf="@id/pok_do_text"
                />
            <TextView
                android:id="@+id/pok_ot"
                style="@style/SecondText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                app:layout_constraintBottom_toBottomOf="@id/pok_ot_text"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@id/pok_ot_text"
                tools:text="500.00" />
            <TextView
                android:id="@+id/pok_do"
                style="@style/SecondText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                app:layout_constraintBottom_toBottomOf="@id/pok_do_text"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@id/pok_do_text"
                tools:text="500.00" />

            <TextView
                android:id="@+id/qty_cube"
                style="@style/SecondText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                app:layout_constraintBottom_toBottomOf="@id/qty_cube_text"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@id/qty_cube_text"
                tools:text="500.00" />

            <TextView
                android:id="@+id/rday_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/days_colon"
                style="@style/ChildText"
                app:layout_constraintTop_toBottomOf="@id/qty_cube_text"
                app:layout_constraintStart_toStartOf="@id/qty_cube_text"
                />
            <TextView
                android:id="@+id/rday"
                style="@style/SecondText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                app:layout_constraintBottom_toBottomOf="@id/rday_text"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@id/rday_text"
                tools:text="500.00" />
            <TextView
                android:id="@+id/cube_day_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/cube_day_colon"
                style="@style/ChildText"
                app:layout_constraintTop_toBottomOf="@id/rday_text"
                app:layout_constraintStart_toStartOf="@id/rday_text"
                />
            <TextView
                android:id="@+id/cube_day_cube"
                style="@style/SecondText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                app:layout_constraintBottom_toBottomOf="@id/cube_day_text"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@id/cube_day_text"
                tools:text="500.00" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>
    <com.google.android.material.divider.MaterialDivider
        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:layout_marginTop="8dp"
        android:color="?attr/colorOnPrimaryContainer"
        app:layout_constraintTop_toBottomOf="@id/card_avg" />

</androidx.constraintlayout.widget.ConstraintLayout>